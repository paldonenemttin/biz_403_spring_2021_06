<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STOUT"
		class="ch.qos.logback.core.ConsoleAppender.class">
		<layout class="ch.qos.logback.classic.PatternLayout.class">
			<Pattern>%d{HH:mm:SSS}{%-5level} - %m%n</Pattern>
		</layout>
	</appender>
	<root level="DEBUG">
		<appender-ref ref="STOUT" />
	</root>
</configuration>