<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.statea.dao.ext.LikeDao">
<insert id="insert" parameterType="LikeVO">
	insert into tbl_like(like_code,like_cncode,like_count)
	values(#{like_code},#{like_cncode},#{like_count})
</insert>
<delete id="delete" parameterType="LikeVO">
	delete from tbl_like
	where like_code = ?
</delete>
</mapper>